<acorn motd="text/main-motd.txt" prompt="text/main-prompt.txt"
	timeoutSeconds="30">
	<command>
		<name>register</name>
		<form post="http://tailfeather.org/tf/register" instructions="text/register-instructions.txt">
			<text name="name">Name</text>
			<email name="email">E-mail Address</email>
		</form>
	</command>
	<command>
		<name>ls</name>
		<name>dir</name>
		<name>ps</name>
		<name>who</name>
		<echo level="error" file="text/command-restricted.txt" />
	</command>
	<command>
		<name>exit</name>
		<name>quit</name>
		<name>bye</name>
		<exit />
	</command>
</acorn>
