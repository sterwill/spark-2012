<acorn motd="text/main-motd.txt" prompt="text/main-prompt.txt"
	promptTimeoutSeconds="30" cps="200">
	<command>
		<name>register</name>
		<form post="http://tailfeather.org/tf/register" instructions="text/register-instructions.txt"
			success="text/register-success.txt">
			<text name="name" prompt="  Name: " />
			<email name="email" prompt="  E-mail Address: " />
		</form>
	</command>
	<command>
		<name>ls</name>
		<name>dir</name>
		<name>ps</name>
		<name>who</name>
		<echo level="error" file="text/command-restricted.txt" />
	</command>
	<command>
		<name>exit</name>
		<name>quit</name>
		<name>bye</name>
		<exit />
	</command>
</acorn>
