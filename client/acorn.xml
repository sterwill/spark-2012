<acorn motd="text/main-motd.txt" prompt="text/main-prompt.txt" promptTimeoutSeconds="30" cps="200">
	<command>
		<name>register</name>
		<register post="http://localhost:8080/api/user" instructions="text/register-instructions.txt" success="text/register-success.txt">
			<text name="name" prompt="  Name: " />
			<email name="email" prompt="  E-mail Address: " />
		</register>
	</command>
	<command>
		<name>ls</name>
		<name>dir</name>
		<name>ps</name>
		<name>who</name>
		<echo level="error" file="text/command-restricted.txt" />
	</command>
	<command>
		<name>exit</name>
		<name>quit</name>
		<name>bye</name>
		<exit />
	</command>
	<scan sound="sounds/sweet.wav" unknownSound="sounds/sour.wav" unknownFile="text/unknown-code.txt" pattern="http://.*/api/user/(.*)" />
</acorn>
